apiVersion: cert-manager.io/v1
kind: Certificate
metadata:
  name: wildcard-spaceterran-com
  namespace: cattle-system
spec:
  secretName: wildcard-spaceterran-com-tls
  issuerRef:
    name: letsencrypt-prod
    kind: ClusterIssuer
  commonName: spaceterran.com
  dnsNames:
  - '*.spaceterran.com'
  - spaceterran.com